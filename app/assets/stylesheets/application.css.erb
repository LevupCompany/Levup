* {
  margin: 0px;
  padding: 0px;
}
img {
  max-width: 100%;
}
a {
  text-decoration: none ;
}
ul {
  list-style: none;
}
input,
select,
button,
textarea {
  outline: none ;
}
button,
input,
select,
textarea {
  border: none;
}
button {
  cursor: pointer;
}
body {
  font-family: 'Open Sans';

}
.screen {
  padding-top: 40px;
}
.screen h2 {
  margin-bottom: 40px;
  text-align: center;
}
#screen1,
.for_screen {
  min-height: 80vh;
  background: url(<%= image_path'screen1.jpg'%>) no-repeat;
  background-size: cover;
  background-position: top;
  padding-bottom: 20px;
  text-align: center;
  position: relative;
  z-index: 1;
  }
  #screen1:before,
  .for_screen:before {
  content: '';
  background: #000;
  opacity: 0.4;
  position: absolute;
  width: 100%;
  z-index: -1;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  }
  #screen1 h1,
  .for_screen h1 {

/* Small Devices, Tablets */
@media only screen and (max-width: 755px) {
  header .wrap {
    display: flex;
    justify-content: flex-start;
    align-items: center;
  }
  a.logo {
    display: block;
    text-align: center;
    margin-right: 10%;
  }
  .nav {
    display: none;
  }
  button.mob {
    display: inline-block;
  }
  .advantages_wrap {
    display: block;
  }
  .advantages_wrap div {
    margin: 0 auto 50px;
  }
  .blog_wrap,
  .tehnology {
    display: block;
  }
  .blog_wrap div,
  .tehnology div {
    margin: 0 auto 20px;
  }
  .advantages_wrap__item {
    width: 100%;
  }
  .advantages_wrap__item__info {
    width: 100%;
  }
  .blogs_wrap__item {
    display: block;
  }
  .blogs_wrap__item img {
    max-width: 100%;
  }
  .crm_nav {
    width: 100%;
  }
}
/* Extra Small Devices, Phones */
@media only screen and (max-width: 520px) {
  #screen_services {
    height: max-content;
    display: block;
  }
  #screen_services div {
    margin: 0 auto 50px;
  }
  .blog_wrap__item {
    width: 100%;
    margin-bottom: 20px;
  }
  #screen_zakaz {
    background-size: 100% 100%;
    height: 200px;
  }
  #screen_zakaz button {
    margin-top: 40px;
  }
  .filtr {
    margin-bottom: 0;
  }
  .filtr_wrap {
    display: block;
  }
  .filtr_wrap a {
    margin: 0 auto 20px;
    width: 100%;
    max-height: 300px;
  }
  .contact_adres {
    display: block;
  }
  .contact_adres .contact_adres__item {
    text-align: center;
  }
  .contact_adres .contact_adres__item img {
    left: 3%;
  }
  .mediafont a {
    font-size: 1rem !important;
  }
  .crm_seting {
    display: block;
  }
  .crm_seting input {
    width: 100%;
  }
  .crm_seting__un,
  .crm_seting__dos,
  .crm_seting__tres {
    width: 100%;
  }
  .crm_seting__btn {
    bottom: -29px;
    padding: 5px 20px;
  }
  .srmnot_item {
    display: block;
  }
  .srmnot_item img {
    display: none;
  }
  .srmnot_item div {
    margin-bottom: 10px;
  }
}
@media only screen and (max-width: 450px) {
  .form {
    display: block;
  }
  .form input {
    display: block;
    width: 100%;
  }
  .footer_wrap {
    display: block;
  }
  .footer_wrap div {
    margin: 0 auto;
  }
  .for_screen {
    min-height: 100px;
    max-height: 300px;
  }
  main.page h1 {
    margin-top: 120px;
    font-size: 3rem;
  }
  #screen_tehnology {
    display: none;
  }
  .crm_nav li a {
    font-size: 0.875rem;
  }
}

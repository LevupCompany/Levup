.form {
  margin: auto;
  position: relative;
  z-index: 2;
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-wrap: wrap;
}
.form input {
  width: 25%;
  background: #eceef4;
  font-size: 1.125rem;
  padding: 5px 10px;
  font-family: 'Open Sans';
  border: 2px solid transparent;
  box-sizing: border-box;
  width: 30%;
  margin-bottom: 20px;
}
.form input[type="email"] {
  margin-right: 0;
}
.form input:focus {
  border: 2px solid #1a89b6;
}
.form textarea {
  display: block;
  background: #eceef4;
  width: 100%;
  height: 200px;
  padding: 5px 10px;
  margin-bottom: 30px;
  border: 2px solid transparent;
  box-sizing: border-box;
}
.form textarea:focus {
  border: 2px solid #1a89b6;
}
.form :-moz-placeholder {
  opacity: .5;
}
.form ::-moz-placeholder {
  opacity: .5;
}
.form :-ms-input-placeholder {
  opacity: .5;
}
.form ::-webkit-input-placeholder {
  opacity: .5;
}
.form button {
  padding: 5px 15px ;
  background: #1a89b6;
  color: #fff;
  font-size: 1.125rem;
  font-family: 'Open Sans';
  border-radius: 3px;
  transition: all 0.4s;
}
.form button:hover {
  opacity: 0.8;
}

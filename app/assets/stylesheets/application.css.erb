}
.function_item span img,
.support_item span img {
  width: 20px;
  height: 20px;
  margin-right: 3px;
}
.innfo {
  box-shadow: 0px 1px 13px 0px rgba(0, 1, 5, 0.15);
  padding: 10px 0 10px 25px;
  width: 70%;
  margin: 20px auto;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.innfo img {
  display: inline-block;
  margin-right: 7px;
}
.innfo p {
  font-size: 0.75rem;
  color: #1faae2;
  display: inline-block;
}
#screen_form {
  background-color: #2b2c2d;
  padding: 20px 0 30px;
  border-bottom: 4px solid #fff;
}
.form_calc {
  color: #FFF;
}
.form_calc input {
  width: 100%;
  height: 30px;
  margin-bottom: 15px;
  box-sizing: border-box;
  padding-left: 10px;
}
.form_calc textarea {
  width: 100%;
  height: 100px;
  margin-bottom: 15px;
  box-sizing: border-box;
  padding-left: 10px;
  padding-top: 5px;
}
@media only screen and (max-width: 965px) {
  .design_wrap {
    display: block;
  }
  .design_item,
  .design_item_chek {
    margin: 0 auto 20px;
  }
  .label_wrap {
    display: flex;
    justify-content: space-around;
    align-items: center;
  }
}
@media only screen and (max-width: 485px) {
  .innfo {
    display: none;
  }
  .label_wrap {
    display: block;
  }
  .label_item {
    display: block;
    margin: 0 auto 15px;
  }
  .function_wrap,
  .support_wrap {
    display: block;
  }
  .function_item,
  .support_item {
    max-width: 100%;
  }
  .form_calc .calc_btn {

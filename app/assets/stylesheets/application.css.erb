}
header {
  padding: 10px 0;
  position: fixed;
  width: 100%;
  z-index: 5;
  transition: all 0.7s;
}
header:hover {
  background: #2b2c2d;
  opacity: .8;
}
header .wrap {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.sticky {
  background: #2b2c2d;
  opacity: .8;
}
a.logo,
a.logout {
  display: inline-block;
}
.nav {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-left: -5%;
}
.nav li {
  padding: 0 5px;
}
.nav li a {
  color: #fff;
  transition: all 0.4s;
}
.nav li a:hover {
  color: #1a89b6;
}
.nav_mob {
  background: #2b2c2d;
  opacity: .8;
  position: absolute;
  top: 100%;
  width: 100%;
  text-align: center;
}
.nav_mob a {
  color: #fff;
  padding: 15px 0;
  display: block;
}
button.mob {
  background: transparent;
  color: #fff;
  font-size: 1.5rem;
  position: absolute;
  right: 10%;
